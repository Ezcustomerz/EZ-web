{
  "name": "ez-web",
  "version": "1.0.0",
  "description": "EZ-Web full-stack application",
  "scripts": {
    "start:backend": "cd backend && .\\venv\\Scripts\\python.exe -m uvicorn main:app --reload --host 0.0.0.0 --port 8000",
    "start:frontend": "cd frontend && npm run dev",
    "start:both": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "install:backend": "cd backend && python -m venv venv && .\\venv\\Scripts\\python.exe -m pip install -r requirements.txt",
    "install:frontend": "cd frontend && npm install",
    "install:all": "npm run install:backend && npm run install:frontend"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "repository": {
    "type": "git",
    "url": "."
  },
  "keywords": [
    "fastapi",
    "react",
    "vite",
    "supabase"
  ],
  "author": "",
  "license": "MIT"
}
